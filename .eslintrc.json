{
  "extends": [
    "next/core-web-vitals",
    "prettier"
  ],
  "rules": {
    "semi": [
      "error",
      "always"
    ],
    "no-use-before-define": "off",
    "@typescript-eslint/no-use-before-define": [
      "off"
    ],
    "react/display-name": "off",
    "react/jsx-curly-brace-presence": [
      "error",
      {
        "props": "always",
        "children": "never"
      }
    ]
  }
}
